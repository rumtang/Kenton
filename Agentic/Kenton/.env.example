# Environment variables for the Agentic Research API
# Copy this file to .env and fill in your API keys

# OpenAI API Key (Required)
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI Vector Store ID (Optional - for document search)
OPENAI_VECTOR_STORE_ID=your_vector_store_id_here

# Weather API Key (Optional - for weather data)
WEATHER_API_KEY=your_weatherapi_key_here

# Financial Modeling Prep API Key (Optional - for financial data)
# Sign up at https://financialmodelingprep.com/developer/docs/
MARKET_DATA_KEY=your_financial_modeling_prep_api_key_here

# Additional Configuration Settings
MODEL=gpt-4.1
DEBUG=false